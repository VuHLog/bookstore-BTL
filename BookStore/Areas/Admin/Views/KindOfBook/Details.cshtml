@model BookStore.Models.KindOfBook

@{
    Layout = "~/Areas/Admin/Views/Shared/Admin/MainLayout.cshtml";
    ViewBag.breadcrumb = "Loại sách";
}


<div id="content-page" class="content-page">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-12">
                <div class="iq-card">
                    <div class="iq-card-body">
                        <div>
                            <h4>Loại sách</h4>
                            <hr />
                            <dl class="row">
                                <dt class="col-sm-2">
                                    ID loại sách
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.KindOfBookId)
                                </dd>
                                <dt class="col-sm-2">
                                    Tên
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Name)
                                </dd>
                            </dl>
                            <a asp-action="Index">Quay lại danh sách loại sách</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>