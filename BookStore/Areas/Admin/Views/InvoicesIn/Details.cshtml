@model BookStore.Models.InvoicesIn

@{
    Layout = "~/Areas/Admin/Views/Shared/Admin/MainLayout.cshtml";
    ViewBag.breadcrumb = "Hoá đơn nhập";
}


<div id="content-page" class="content-page">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-12">
                <div class="iq-card">
                    <div class="iq-card-body">
                        <div>
                            <h4>Hoá đơn nhập</h4>
                            <hr />
                            <dl class="row">
                                <dt class="col-sm-2">
                                    ID hoá đơn nhập
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.InvoicesInId)
                                </dd>
                                <dt class="col-sm-2">
                                    Ngày nhập
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Date)
                                </dd>
                                <dt class="col-sm-2">
                                    Nhà xuất bản
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Publisher.Name)
                                </dd>
                                <dt class="col-sm-2">
                                    Nhân viên
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Employee.Name)
                                </dd>
                            </dl>
                            <a asp-action="Index">Quay lại danh sách hoá đơn nhập</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>