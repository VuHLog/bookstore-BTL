@model BookStore.Models.InvoicesOut

@{
    Layout = "~/Areas/Admin/Views/Shared/Admin/MainLayout.cshtml";
    ViewBag.breadcrumb = "Xuất hoá đơn";
}


<div id="content-page" class="content-page">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-12">
                <div class="iq-card">
                    <div class="iq-card-body">
                        <div>
                            <h4>Xuất hoá đơn</h4>
                            <hr />
                            <dl class="row">
                                <dt class="col-sm-2">
                                    ID xuất hoá đơn
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.InvoicesOutId)
                                </dd>
                                <dt class="col-sm-2">
                                    Ngày xuất
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Date)
                                </dd>
                                <dt class="col-sm-2">
                                    Khách hàng
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Customer.Name)
                                </dd>
                                <dt class="col-sm-2">
                                    Nhân viên
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Employee.Name)
                                </dd>
                            </dl>
                            <a asp-action="Index">Quay lại danh sách xuất hoá đơn</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>